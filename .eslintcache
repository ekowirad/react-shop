redux/cart/cart.action'\r\n\r\nfunction SigninPage() {\r\n    /**\r\n     * VARIABLE SECTION \r\n     */\r\n    const initData = {\r\n        email: '',\r\n        password: '',\r\n    }\r\n    const dispatch = useDispatch();\r\n    const history = useHistory()\r\n    const [data, setData] = useState(initData)\r\n    const [load, setLoad] = useState(false)\r\n\r\n    /**\r\n     * FUNCTION SECTION \r\n     */\r\n    const handleChange = (event) => {\r\n        const { name, value } = event.target\r\n        setData({ ...data, [name]: value })\r\n    }\r\n\r\n    const signInWithEmail = async () => {\r\n        const { email, password } = data\r\n\r\n        setLoad(true)\r\n        try {\r\n            const { user } = await auth.signInWithEmailAndPassword(email, password)\r\n            dispatch(fetchCart(user))\r\n            dispatch(setCurrentUser(user))\r\n            setData(initData)\r\n            setLoad(false)\r\n            history.push(\"/\")\r\n\r\n        } catch (e) {\r\n            setLoad(false)\r\n            console.log(e);\r\n            alert(e.message)\r\n\r\n        }\r\n\r\n\r\n    }\r\n\r\n    const googleSigninAct = async () => {\r\n        setLoad(true)\r\n        try {\r\n            const { user } = await googleSignin()\r\n            await storeNewUser(user)\r\n            dispatch(fetchCart(user))\r\n            dispatch(setCurrentUser(user))\r\n            setData(initData)\r\n            setLoad(false)\r\n            history.push(\"/\")\r\n\r\n        } catch (e) {\r\n            setLoad(false)\r\n            console.log(e);\r\n            alert(e.message)\r\n        }\r\n    }\r\n\r\n    return (\r\n        <SigninSignupWrap>\r\n            <div className=\"inner-top\">\r\n                <h1>Sign In</h1>\r\n                <FormInput onChange={handleChange} name=\"email\" type=\"email\" value={data.email} placeholder=\"example@mail.com\" label=\"Email\"></FormInput>\r\n                <FormInput onChange={handleChange} name=\"passw